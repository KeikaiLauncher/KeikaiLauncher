<resources>
    <!-- This string is the key used to retrieve the value for rotation preference. -->
    <string name="pref_key_allow_rotation" translatable="false">pref_allow_rotation</string>

    <!-- This string is the key used to retrieve the value for autokeyboard preference. -->
    <string name="pref_key_auto_keyboard" translatable="false">pref_autokeyboard</string>

    <!-- This string is the key used to retrieve the value for icon preference. -->
    <string name="pref_key_disable_icons" translatable="false">pref_disable_icons</string>

    <!--
         This string is the key used to retrieve the value for the 270 degree orientation with the
         navigation bar.
    -->
    <string name="pref_key_landscape_270" translatable="false">
        settings_landscape_270
    </string>

    <!--
     This string is the key used to retrieve the value for the 90 degree orientation with the
     navigation bar.
    -->
    <string name="pref_key_landscape_90" translatable="false">
        settings_landscape_90
    </string>

    <!-- This string is the key used to retrieve the intent for system usage permissions. -->
    <string name="pref_key_modify_usage_statistics" translatable="false">
        modify_usage_statistics
    </string>

    <!-- This string is the key used to retrieve the value of launchable ordering. -->
    <string name="pref_key_preferred_order" translatable="false">pref_app_preferred_order</string>

    <!-- This string is a possible value for alphabetical launchable ordering. -->
    <string name="pref_value_preferred_order_alpha" translatable="false">alphabetical</string>

    <!-- This string is a possible value for ordering launchables by recent usage. -->
    <string name="pref_value_preferred_order_recent" translatable="false">recent</string>

    <!-- This string is a possible value for ordering launchables by frequency of usage. -->
    <string name="pref_value_preferred_order_usage" translatable="false">usage</string>
</resources>
